{
  "name": "roundpartner/cloud",
  "description": "Cloud Services",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Tom Lorentsen",
      "email": "tom@roundpartner.co.uk"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/roundpartner/configuration"
    },
    {
      "type": "git",
      "url": "https://github.com/roundpartner/verify-hash"
    }
  ],
  "autoload": {
    "psr-4": {
      "RoundPartner\\Cloud\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RoundPartner\\Test\\": "tests/"
    },
    "psr-0": {
      "OpenCloud": ["vendor/rackspace/php-opencloud/tests/"]
    }
  },
  "require": {
    "rackspace/php-opencloud": "^1.16",
    "roundpartner/verify-hash": "dev-master"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "2.*",
    "phpunit/phpunit": "^4.8",
    "roundpartner/configuration": "dev-master"
  }
}
